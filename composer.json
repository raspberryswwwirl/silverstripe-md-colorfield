{
    "name": "raspberryswwwirl/silverstripe-md-colorfield",
    "description": "Proof-of-concept for Material Design colorpicker built with Vue.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "RaspberrySwwwirl",
            "email": "hello@raspberryswwwirl.com"
        }
    ],
    "require": {
        "silverstripe/framework": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "RS\\": "src/RS"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "expose": [
            "client/dist"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    },
    "require-dev": {
        "wernerkrauss/silverstripe-rector": "dev-main"
    }
}
